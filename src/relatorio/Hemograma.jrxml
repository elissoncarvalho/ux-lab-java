	<queryString>
		<![CDATA[SELECT PEDIDO.ID_N_PEDIDO, pedido.DATA_PEDIDO, PEDIDO.CONVENIO, cliente.NOME, hemograma.HEMACIAS, hemograma.HEMOGLOBINA, hemograma.HEMATOCRITO, hemograma.VCM, hemograma.HCM, hemograma.CHCM, hemograma.RDW, hemograma.LEUCOCITOS, hemograma.NEUTROFILOS, hemograma.SEGMENTADOS, hemograma.BASTONETES, hemograma.EOSINOFILOS, hemograma.LINFOCITOS, hemograma.MONOCITOS, hemograma.LEUCOCITOS1, hemograma.NEUTROFILOS1, hemograma.SEGMENTADOS1, hemograma.BASTONETES1, hemograma.EOSINOFILOS1, hemograma.LINFOCITOS1, hemograma.MONOCITOS1, hemograma.CONTAGEM_PLAQUETAS
FROM PEDIDO 
	INNER JOIN cliente 
    	ON cliente.DELET = 0 AND cliente.ID_CLIENTE = pedido.COD_CLIENTE_FK 
    INNER JOIN hemograma 
    	on hemograma.ID_PEDIDO_FK = pedido.ID_N_PEDIDO 
WHERE pedido.DELET = 0 AND PEDIDO.ID_N_PEDIDO = 1 
GROUP BY pedido.ID_N_PEDIDO]]>
	</queryString>
